{
  "name": "@d-fischer/rate-limiter",
  "version": "0.7.4",
  "description": "Rate limit your requests.",
  "keywords": [
    "rate",
    "limit",
    "http",
    "request"
  ],
  "main": "lib",
  "types": "lib",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./es/index.mjs"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/d-fischer/rate-limiter.git"
  },
  "author": "Daniel Fischer <daniel@d-fischer.dev>",
  "license": "MIT",
  "dependencies": {
    "@d-fischer/logger": "^4.2.1",
    "@d-fischer/promise.allsettled": "^2.0.2",
    "@d-fischer/shared-utils": "^3.2.0",
    "@types/node": "^12.12.5",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@d-fischer/eslint-config": "^6.1.8",
    "eslint": "^8.30.0",
    "husky": "^4.3.6",
    "lint-staged": "^13.1.0",
    "prettier": "^2.2.1",
    "tsukuru": "^0.8.0-pre.11",
    "typescript": "~4.9.4"
  },
  "files": [
    "LICENSE",
    "README.md",
    "lib",
    "es"
  ],
  "scripts": {
    "lint": "eslint --ext js,ts src",
    "prettier:check": "prettier --check 'src/**'",
    "prettier:fix": "prettier --write 'src/**'",
    "build": "tsukuru",
    "rebuild": "tsukuru --clean"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,json}": "prettier --write",
    "*.{ts}": "eslint --fix"
  }
}
