{
  "name": "@d-fischer/rate-limiter",
  "version": "0.2.4",
  "description": "Rate limit your requests.",
  "keywords": [
    "rate",
    "limit",
    "http",
    "request"
  ],
  "main": "lib",
  "types": "lib",
  "module": "es",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./es/index.mjs"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/d-fischer/rate-limiter.git"
  },
  "author": "Daniel Fischer <daniel@d-fischer.dev>",
  "license": "MIT",
  "dependencies": {
    "@d-fischer/logger": "^2.0.0",
    "@d-fischer/promise.allsettled": "^2.0.1",
    "@types/node": "^12.12.5",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@d-fischer/eslint-config": "^1.0.11",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "tsukuru": "^0.4.4",
    "typescript": "~3.8.3"
  },
  "files": [
    "LICENSE",
    "README.md",
    "lib",
    "es"
  ],
  "scripts": {
    "lint": "eslint --ext js,ts src",
    "prettier:check": "prettier --check 'src/**'",
    "prettier:fix": "prettier --write 'src/**'",
    "build": "tsukuru",
    "rebuild": "tsukuru --clean"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ]
  }
}
